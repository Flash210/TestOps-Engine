{
  "default": {
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "paths": ["src/tests/features"],
    "dryRun": false,
    "publishQuiet": true,
    "require": ["src/tests/steps/**/*.ts", "src/hooks/hooks.ts"],
    "requireModule": ["ts-node/register"],
    "format": [
      "progress-bar",

      "html:test-results/cucumber-report.html",
      "json:test-results/cucumber-report.json",
      "rerun:@rerun.txt"
    ],
    "parallel": 2,
    "tags": "not @skip"
  },

  "rerun": {
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "dryRun": false,
    "publishQuiet": true,
    "require": ["src/tests/steps/**/*.ts", "src/hooks/hooks.ts"],
    "requireModule": ["ts-node/register"],
    "format": [
      "progress-bar",

      "html:test-results/cucumber-report.html",
      "json:test-results/cucumber-report.json",
      "rerun:@rerun.txt"
    ],
    "parallel": 2,
    "tags": "not @skip"
  }
}
